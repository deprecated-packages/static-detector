{
    "name": "symplify/static-detector",
    "description": "Detect static calls in your project",
    "license": "MIT",
    "bin": [
        "bin/static-detector"
    ],
    "require": {
        "php": ">=8.0",
        "symfony/dependency-injection": "^5.3",
        "symfony/console": "^5.3",
        "nikic/php-parser": "4.10.5",
        "symfony/http-kernel": "^5.3",
        "symplify/symplify-kernel": "^9.4.1",
        "symplify/astral": "^9.4.1",
        "symplify/package-builder": "^9.4.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\StaticDetector\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\StaticDetector\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "9.5-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
